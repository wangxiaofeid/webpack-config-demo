{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ], 
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "@babel/plugin-transform-runtime",
    "lodash",
    ["import", { "libraryName": "antd", "style": true }]
  ]
}